<div class="ft-box col-xs-10 col-xs-offset-1">
  <h1>How can we assist you today?</h1>
  <h2><% @requests.each do |request| %></h2>
  <%= request.name %>
  <%= request.description %>
  <%= request.phone %>
  <%= request.email %>
  <%= request.created_at %>

  <% if request.user.present? %>
    <small><%= request.user.email %></small>
  <% end %>
  <% end %>
</div>
